package com.Demo;

public class Student {

	private String name = "Haswanth";
	private int id = 1;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public void studentDetails(String name, int id) {
		System.out.println("Student Details: ");
		this.id = id;
		this.name = name;
		System.out.println("Id:" + " " + id + " Name " + " " + name);

	}

	public static void studentDetials(String name, int id, String country) {
		System.out.println("Student Details: ");
		System.out.println("Id:" + " " + id + " Name: " + " " + name + " Country: " + " " + country);

	}
}
