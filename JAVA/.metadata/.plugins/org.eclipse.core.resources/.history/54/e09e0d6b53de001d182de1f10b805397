package com.Project;

public class Student {

	// Encapsulation
	private int id; // private, we cannot access it from outside this class:
	private String name;

	
	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public static void main(String args[]){



