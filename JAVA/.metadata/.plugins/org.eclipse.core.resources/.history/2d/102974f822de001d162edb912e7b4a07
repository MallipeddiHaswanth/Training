package com.Demo;

class StudentDetials extends Student {

	public void Display2(int id, String name) {
		System.out.println("Displaying from Parent Class:");
		this.Display(id, name);

	}

	public static void main(String[] args) {

		Student Student_1 = new Student();
		StudentDetials Student_2 = new StudentDetials();

		Student_1.Display(1, "Haswanth");
		Student_2.Display2(2, "Ram");

		// System.out.println(Student_1.id + " , " + Student_1.name + " , " +
		// Student.clg);

	}
}
