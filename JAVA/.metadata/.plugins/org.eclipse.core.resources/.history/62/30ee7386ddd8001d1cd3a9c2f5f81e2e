package com.day1;

public class Inhertiance1 {
	int roll_no;
	String name, lname;
	String course = "Bachelors";

	Inhertiance1() {
		// super();
		System.out.println("Students ");

	}

	public Inhertiance1(int roll_no, String name, String lname, String course) {
		super();
		this.roll_no = roll_no;
		this.name = name;
		this.lname = lname;
	}

	public void display() {
		System.out.println(roll_no + name + lname + course);
	}

}
